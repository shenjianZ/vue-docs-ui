{
  "name": "vue-docs-ui",
  "version": "1.0.1",
  "description": "A modern documentation UI component library built with Vue 3. Create beautiful documentation websites with YAML configuration and Markdown rendering - ready to use out of the box.",
  "main": "dist/vue-docs-ui.umd.js",
  "module": "dist/vue-docs-ui.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README-zh.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-docs-ui.es.js",
      "require": "./dist/vue-docs-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.js",
    "preview": "vite preview",
    "serve": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build:lib",
    "publish:check": "node scripts/publish.js",
    "publish:dry": "npm publish --dry-run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@vueuse/core": "^10.0.0",
    "js-yaml": "^4.1.0",
    "lucide-vue-next": "^0.511.0",
    "markdown-it": "^13.0.0",
    "markdown-it-anchor": "^8.6.0",
    "markdown-it-prism": "^2.3.0",
    "prismjs": "^1.29.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "sass": "^1.66.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^1.8.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "documentation",
    "docs",
    "markdown",
    "yaml",
    "ui-library",
    "component-library",
    "static-site-generator",
    "docs-generator",
    "documentation-framework",
    "static-site",
    "technical-documentation",
    "out-of-the-box",
    "zero-config",
    "responsive-design",
    "typescript"
  ],
  "author": "Vue Docs UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shenjianZ/vue-docs-ui.git"
  },
  "bugs": {
    "url": "https://github.com/shenjianZ/vue-docs-ui/issues"
  },
  "homepage": "https://github.com/shenjianZ/vue-docs-ui#readme"
}
